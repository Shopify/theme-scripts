{
  "name": "@shopify/theme-sections",
  "version": "1.0.0-alpha.4",
  "description":
    "A framework that helps developers build Shopify Theme sections that play well with the Shopify Theme Editor",
  "main": "dist/sections.cjs.js",
  "modules": "dist/sections.es5.js",
  "repository":
    "https://github.com/Shopify/theme-scripts/tree/master/packages/theme-sections",
  "keywords": ["slate"],
  "author": "Shopify Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shopify/theme-scripts/issues"
  },
  "homepage": "https://github.com/Shopify/theme-scripts#readme",
  "peerDependencies": {
    "jquery": "3.x",
    "lodash-es": "4.x"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:es5 & npm run build:cjs",
    "build:es5":
      "mkdir -p dist && ../../node_modules/.bin/babel sections.js --out-file dist/sections.es5.js",
    "build:cjs":
      "mkdir -p dist && ../../node_modules/.bin/babel --no-babelrc sections.js --out-file dist/sections.cjs.js --presets=shopify"
  }
}
