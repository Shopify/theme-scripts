{
  "name": "@shopify/theme-a11y",
  "version": "1.0.0-alpha.3",
  "description":
    "A library of useful functions that help make your theme more accessible.",
  "main": "dist/a11y.cjs.js",
  "modules": "dist/a11y.es5.js",
  "repository":
    "https://github.com/Shopify/theme-scripts/tree/master/packages/theme-a11y",
  "keywords": ["slate", "a11y"],
  "author": "Shopify Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shopify/theme-scripts/issues"
  },
  "homepage": "https://github.com/Shopify/theme-scripts#readme",
  "peerDependencies": {
    "jquery": "3.x"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build:es5 & npm run build:cjs",
    "build:es5":
      "mkdir -p dist && ../../node_modules/.bin/babel a11y.js --out-file dist/a11y.es5.js",
    "build:cjs":
      "mkdir -p dist && ../../node_modules/.bin/babel --no-babelrc a11y.js --out-file dist/a11y.cjs.js --presets=shopify"
  }
}
